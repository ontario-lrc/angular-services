{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "load-script": {
      "projectType": "library",
      "root": "projects/load-script",
      "sourceRoot": "projects/load-script/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/load-script/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/load-script/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/load-script/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/load-script/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "mat-tab-change": {
